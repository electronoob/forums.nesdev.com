ndler(0x8000, 0xFFFF, CartBR);<br />&nbsp; &nbsp;Sync();<br />}<br /><br />static void M277Reset(void) {<br />&nbsp; &nbsp;reg = 0;<br />&nbsp; &nbsp;Sync();<br />}<br /><br /><br />static void StateRestore(int version) {<br />&nbsp; &nbsp;Sync();<br />}<br /><br />void Mapper277_Init(CartInfo *info) {<br />&nbsp; &nbsp;info-&gt;Power = M277Power;<br />&nbsp; &nbsp;info-&gt;Reset = M277Reset;<br />&nbsp; &nbsp;<br />&nbsp; &nbsp;GameStateRestore = StateRestore;<br /><br />&nbsp; &nbsp;AddExState(&amp;StateRegs, ~0, 0, 0);<br />}<br /><br /></div></div><br />thank you ,but batte city enter game have bug. in fc not have.</div>

