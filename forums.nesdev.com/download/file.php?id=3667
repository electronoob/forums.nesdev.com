;VARIABLE DEFINITIONS
;00-0F tile values
;10-1F vertical travel speeds
;20-2F horizontal travel speeds
;30-3F no switch flag
;40 press-unpress flag
;41 shift animation countdown
;42 random seed
;43 modded random seed 00-7F
;44 number of blank cells at any time
;45 random seed modded by number of cells / selection of nth blank cell
;46 random nth blank cell
;47 new block animation countdown
;48 restart animation countdown
;49 counter for LoadBlock
;4A temporary variable to store X in while X is being used to construct a score
;4B temporary variable to store Y in while Y is being used to construct a score
;4C flag to update the PPU scores
;4D plus animation countdown
;4E palette color for current fade phase
;4F update tracks flag
;50 value of (new) block 0-E
;51 value of the tile that was built during a join, used to construct a score
;52 location of (new) block 0-F
;53 tempvar PPU_Program
;54 tempvar PPU_Value
;55 countdown for how many PPU line loops to do before quitting to beat VBlank
;60-64 score of last individual blocks added
;70-74 score of last entire slide
;79-7E nametable positions of added score
;80-84 current score
;90-94 high score
;A0 song total position
;A1 song page position
;A2 music start switch
;A3 music on/off switch
;AA,AB indirect addresses of current PPU_Program
;B0-B6 loaded track position tracks 0-6
;BA,BB indirect addresses of current PPU_Value
;C0-C4 current instrument counter pulse, on counter for triangle
;CA,CB indirect address choices to be loaded into BA,BB
;D0-D9 indirect addresses of track locations
;DA tempvar for indirect address
;E0-E9 indirect address of instrument locations
;EA,EB indirect address of bankL
;EC,ED indirect address of bankR
;FF controller state

  .inesmap 4   ; mapper 4 = MMC3
  .inesprg 8   ; # of 16KB PRG banks 128KB
  .ineschr 32  ; # of 8KB CHR banks  256KB
  .inesmir 3   ; background mirroring

  .bank 15
  .org $FFFA     ;first of the three vectors starts here
  .dw NMI        ;when an NMI happens (once per frame if enabled) the processor will jump to the label NMI
  .dw RESET      ;when the processor first turns on or is reset, it will jump to the label RESET
  .dw 0          ;external interrupt IRQ is not used in this tutorial

  .org $E000

RESET:
  LDA #$46
  STA $8000    ; fix $8000-9FFF to bank 14, and choose page 6 ($C000-DFFF)
  LDX #$02
  LDA #$42
  STA $8000
  STX $8001
  LDX #$03
  LDA #$43
  STA $8000
  STX $8001
  LDX #$05
  LDA #$44
  STA $8000
  STX $8001
  LDX #$48
  LDA #$45
  STA $8000
  STX $8001
  SEI          ; disable IRQs
  CLD          ; disable decimal mode
  LDX #$40
  STX $4017    ; disable APU frame IRQ
  LDX #$FF
  TXS          ; Set up stack
  INX
  STX $2000    ; disable NMI
  STX $2001    ; disable rendering
  STX $4010    ; disable DMC IRQs
  
ClearMemory:
  LDA #$00
  STA $0000,X
  STA $0100,X
  STA $0300,X
  STA $0400,X
  STA $0500,X
  STA $0600,X
  STA $0700,X
  LDA #$FF
  STA $0200,X
  INX
  BNE ClearMemory
  
ClearNametables:
  LDA $2002
  LDA #$20
  STA $2006
  LDA #$00
  STA $2006 ; PPU latched to $2000
  STA $2000 ; to set PPU address increment to single byte (just in case)
  TAX
  TAY
NameLoop:
  STA $2007
  INX
  BNE NameLoop
  INY
  CPY #$10
  BNE NameLoop

  LDA PPU_Value0,X
  STA $CA
  LDA PPU_Value1,X
  STA $CB
  INX
  LDA PPU_Value0,X
  STA $CC
  LDA PPU_Value1,X
  STA $CD
  INC $4F
  JSR TransferPPU
  JSR TransferPPU
  INC $56
  
  LDA #$82
  STA $A0
  LDA #$77
  STA $A1
  JSR Initial

  LDA #$01
  STA $A3
  LDA #$0F
  STA $4015
  LDA #$80
  STA $A001
  LDA #$01
  STA $4C
  LDA #$90
  STA $E1

  LDA #%10010000   ; enable NMI, sprites from Pattern Table 0, background from Pattern Table 1
  STA $2000
  LDA #%00011110   ; enable sprites, enable background, no clipping on left side
  STA $2001

  JSR Placer

Forever:
  JMP Forever

NMI:
  INC $42
  JSR TransferPPU
  JSR UpdatePPU
  JSR People
  JSR AnimateShift
  JSR AnimateNewBlock
  JSR AnimateRestart
  JSR ReadController
  JSR Music
  RTI

AddAndRollover:            ; needs X - stands for first location that needs to be added to the next location
  LDY #$00
AddLoop:
  LDA $10,X
  CLC
  ADC $00,X
  STA $10,X
  INX
  INY
  CPY #$05
  BNE AddLoop
DigitRollover:
  CPY #$00
  BEQ DigitRolloverDone
  DEX
  DEY
  LDA $10,X
  CMP #$0A
  BMI DigitRollover
  INC $0F,X
  SEC
  SBC #$0A
  STA $10,X
  JMP DigitRollover
DigitRolloverDone:
  RTS

AnimateNewBlock:
  LDA $47
  BNE BeginAnimateNewBlock
  RTS
BeginAnimateNewBlock:
  DEC $47
  JSR Ax10
  TAX
  LDA #$00
  STA $0202,X
  LDA #$40
  STA $0206,X
  LDA #$80
  STA $020A,X
  LDA #$C0
  STA $020E,X
  LDA $47
  BNE AnimateNewBlockContinue
  JSR UpdateTiles
  RTS
AnimateNewBlockContinue:
  LDA #$46
  LDY $50
  CPY #$02
  BNE ValueNot2
  LDA #$56
ValueNot2:
  SEC
  SBC $47
  STA $0201,X
  STA $0205,X
  STA $0209,X
  STA $020D,X
  RTS

AnimateRestart:
  LDA $48
  BNE BeginAnimateRestart
  RTS
BeginAnimateRestart:
  DEC $48
  BNE AnimateRestartMore
  LDA #$00
  TAX
ClearLoop:
  STA $00,X
  INX
  CPX #$10
  BNE ClearLoop
  JSR NewBlock
  JSR NewBlock
  JSR UpdateTiles
  INC $40
  LDX #$70
  JSR ClearScore
  LDX #$80
  JSR ClearScore
  INC $4C
  RTS
AnimateRestartMore:
  LDX #$00
AnimateRestartLoop:
  DEC $0200,X
  DEC $0204,X
  INC $0208,X
  INC $020C,X
  INC $0202,X
  INC $0206,X
  INC $020A,X
  INC $020E,X
  DEC $0203,X
  INC $0207,X
  DEC $020B,X
  INC $020F,X
  TXA
  CLC
  ADC #$10
  TAX
  BNE AnimateRestartLoop
  LDA #$00
  RTS

AnimateShift:
  LDA $41
  BNE BeginAnimateShift
  RTS
BeginAnimateShift:
  LDX #$00
  LDY #$00
AnimateIndividual:
  LDA $0200,X
  CLC
  ADC $10,Y
  STA $0200,X
  STA $0204,X
  CLC
  ADC #$08
  STA $0208,X
  STA $020C,X
  LDA $0203,X
  CLC
  ADC $20,Y
  STA $0203,X
  STA $020B,X
  CLC
  ADC #$08
  STA $0207,X
  STA $020F,X
  TXA
  CLC
  ADC #$10
  TAX
  INY
  CPX #$00
  BNE AnimateIndividual
  DEC $41
  LDA $41
  BNE AnimateShiftEnd
  JSR UpdateTiles
  JSR NewBlock
  JSR IndividualTile
  LDA #$08
  STA $47
  JSR UpdateScore
AnimateShiftEnd:
  RTS

Ax10:
  LDA $52
  ASL A
  ASL A
  ASL A
  ASL A
  RTS

ClearFlagsAndTravel:
  LDA #$00
  LDX #$00
ClearFlagsAndTravelLoop:
  STA $10,X
  INX
  CPX #$30
  BNE ClearFlagsAndTravelLoop
  RTS

ClearScore:
  LDA #$00
  STA $00,X
  STA $01,X
  STA $02,X
  STA $03,X
  STA $04,X
  RTS

HighScore:
  LDA $80
  CMP $6000
  BEQ Compare1
  BPL UpdateHighScore
  RTS
Compare1:
  LDA $81
  CMP $6001
  BEQ Compare2
  BPL UpdateHighScore
  RTS
Compare2:
  LDA $82
  CMP $6002
  BEQ Compare3
  BPL UpdateHighScore
  RTS
Compare3:
  LDA $83
  CMP $6003
  BEQ Compare4
  BPL UpdateHighScore
  RTS
Compare4:
  LDA $84
  CMP $6004
  BPL UpdateHighScore
  RTS
UpdateHighScore:
  LDA $80
  STA $6000
  LDA $81
  STA $6001
  LDA $82
  STA $6002
  LDA $83
  STA $6003
  LDA $84
  STA $6004
UpdateHighScoreSkip:
  RTS

IndividualTile:
  JSR Ax10
  TAY
  LDA $52
  CLC
  ROR A
  CLC
  ROR A
  TAX
  LDA spritevertical,X
  STA $0200,Y
  STA $0204,Y
  CLC
  ADC #$08
  STA $0208,Y
  STA $020C,Y
  LDA $50
  BNE NotZero
  LDA #$FF
  STA $0200,Y
  STA $0204,Y
  STA $0208,Y
  STA $020C,Y
NotZero:
  LDA $52                 ; horizontal
  AND #%00000011
  TAX
  LDA spritehorizontal,X
  STA $0203,Y
  STA $020B,Y
  CLC
  ADC #$08
  STA $0207,Y
  STA $020F,Y
  LDX $50                 ; tile
  LDA spritetile,X
  STA $0201,Y
  TAX
  INX
  TXA
  STA $0205,Y
  TAX
  INX
  TXA
  STA $0209,Y
  TAX
  INX
  TXA
  STA $020D,Y
  LDX $50                 ; palette
  LDA spritepalette,X
  STA $0202,Y
  STA $0206,Y
  STA $020A,Y
  STA $020E,Y
  RTS

Music:
  LDA $A2
  BNE BeginMusic
  RTS
BeginMusic:
  LDY $B0
  LDA [$D0],Y
  CMP $A1
  BNE SkipInstChange0
  INC $B0
  INC $B0
  LDA #$00
  STA $C0
  INY
  LDA [$D0],Y
  TAX
  LDA instloc0,X
  STA $E0
  LDA instloc1,X
  STA $E1
  LDA #$5E
  STA $4002
  LDA #$00
  STA $4003
SkipInstChange0:
  LDY $C0
  LDA [$E0],Y
  CMP #$FF
  BEQ BeginTrack1
  INC $C0
  LDA [$E0],Y
  STA $4000
BeginTrack1:
  LDY $B1
  LDA [$D2],Y
  CMP $A1
  BNE SkipInstChange1
  INC $B1
  INC $B1
  INC $B1
  LDA #$00
  STA $C1
  INY
  LDA [$D2],Y
  TAX
  LDA instloc0,X
  STA $E2
  LDA instloc1,X
  STA $E3
  INY
  LDA [$D2],Y
  TAX
  LDA notefreq0,X
  STA $4006
  LDA notefreq1,X
  STA $4007
  LDX $A0
  LDA open0,X
  STA $F2
  LDA open1,X
  STA $F4
  INC $F7
SkipInstChange1:
  LDY $C1
  LDA [$E2],Y
  CMP #$FF
  BEQ BeginTrack2
  STA $4004
  INC $C1
  LDA [$E2],Y
  CMP #$70
  BNE BeginTrack2
  LDX $A0
  LDA closed0,X
  STA $F2
  LDA closed1,X
  STA $F4
  INC $F7
BeginTrack2:
  LDY $B2
  LDA [$D4],Y
  CMP $A1
  BNE SkipNewTone2
  INC $B2
  INC $B2
  INC $B2
  INY
  LDA [$D4],Y
  STA $C2
  INY
  LDA [$D4],Y
  TAX
  LDA notefreq0,X
  STA $400A
  LDA notefreq1,X
  STA $400B
  LDA #$81
  STA $4008
SkipNewTone2:
  LDA $C2
  BNE BeginTrack3
  LDA #$00
  STA $4008
  INC $C2
BeginTrack3:
  DEC $C2
  LDY $B3
  LDA [$D6],Y
  CMP $A1
  BNE SkipInstChange3
  INC $B3
  INC $B3
  INC $B3
  LDA #$00
  STA $C3
  INY
  LDA [$D6],Y
  TAX
  LDA instloc0,X
  STA $E6
  LDA instloc1,X
  STA $E7
  INY
  LDA [$D6],Y
  STA $400E
SkipInstChange3:
  LDY $C3
  LDA [$E6],Y
  CMP #$FF
  BEQ BeginTrack4
  LDA [$E6],Y
  STA $400C
  LDA #$FF
  STA $400F
  INC $C3
BeginTrack4:
  LDY $B4
  LDA [$D8],Y
  CMP $A1
  BNE TracksDone
  INC $B4
  INC $B4
  INY
  LDA [$D8],Y
  LDY #$46
  STY $8000      ; fix $8000-9FFF to bank 14, and choose page 6 ($C000-DFFF)
  STA $8001
  LDA #$0F
  STA $4010
  LDA #$FF
  STA $4013
  LDA $A3
  BEQ TracksDone
  LDA #$0F
  STA $4015
  LDA #$1F
  STA $4015
TracksDone:
  INC $A1
  LDA $A1
  CMP #$78
  BEQ MusicContinue
  RTS
MusicContinue:
  INC $A0
  LDA $A0
  CMP #$83
  BNE UpdateTracks
  LDA #$00
  STA $A0
UpdateTracks:
  LDA #$00
  STA $A1
Initial:
  LDA #$00
  STA $B0
  STA $B1
  STA $B2
  STA $B3
  STA $B4
  STA $B5
  STA $B6
  LDX $A0

  LDY track0,X
  LDA trackloc00,Y
  STA $D0
  LDA trackloc01,Y
  STA $D1
  LDY track1,X
  LDA trackloc10,Y
  STA $D2
  LDA trackloc11,Y
  STA $D3
  LDY track2,X
  LDA trackloc20,Y
  STA $D4
  LDA trackloc21,Y
  STA $D5
  LDY track3,X
  LDA trackloc30,Y
  STA $D6
  LDA trackloc31,Y
  STA $D7
  LDY track4,X
  LDA trackloc40,Y
  STA $D8
  LDA trackloc41,Y
  STA $D9
  LDY TilesL,X
  LDA TilesL0,Y
  STA $CA
  LDA TilesL1,Y
  STA $CB
  LDY TilesR,X
  LDA TilesR0,Y
  STA $CC
  LDA TilesR1,Y
  STA $CD
  INC $4F
  LDY BankL,X
  LDA BankL0,Y
  STA $EA
  LDA BankL1,Y
  STA $EB
  LDY BankR,X
  LDA BankR0,Y
  STA $EC
  LDA BankR1,Y
  STA $ED
  STA $F0
  LDA mouthtable0,X
  STA $F1
  LDA mouthtable1,X
  STA $F3
  RTS

NewBlock:
  LDX #$00
  LDY #$00
CountBlanks:
  LDA $00,X
  BNE CounterSkip
  INY
CounterSkip:
  INX
  CPX #$10
  BNE CountBlanks
  STY $44                 ; number of blanks
  LDA $42                 ; game counter
  AND #%01111111
  STA $43                 ; game counter modded
Mod:
  CMP $44
  BMI SubtracterSkip
  SEC
  SBC $44
  JMP Mod
SubtracterSkip:
  STA $45                 ; nth blank cell - 0 is first
  STA $46
  TAX
  LDY $44
  CPY #$01
  BNE Blank2
  LDA #$00
Blank2:
  CPY #$02
  BNE Blank3
  LDA random2,X
Blank3:
  CPY #$03
  BNE Blank4
  LDA random3,X
Blank4:
  CPY #$04
  BNE Blank5
  LDA random4,X
Blank5:
  CPY #$05
  BNE Blank6
  LDA random5,X
Blank6:
  CPY #$06
  BNE Blank7
  LDA random6,X
Blank7:
  CPY #$07
  BNE Blank8
  LDA random7,X
Blank8:
  CPY #$08
  BNE Blank9
  LDA random8,X
Blank9:
  CPY #$09
  BNE BlankA
  LDA random9,X
BlankA:
  CPY #$0A
  BNE BlankB
  LDA randomA,X
BlankB:
  CPY #$0B
  BNE BlankC
  LDA randomB,X
BlankC:
  CPY #$0C
  BNE BlankD
  LDA randomC,X
BlankD:
  CPY #$0D
  BNE BlankE
  LDA randomD,X
BlankE:
  CPY #$0E
  BNE BlankF
  LDA randomE,X
BlankF:
  CPY #$0F
  BNE Blank10
  LDA randomF,X
Blank10:
  CPY #$10
  BNE BlankEnd
  LDA random10,X
BlankEnd:
  STA $46
  LDX #$FF
  LDY #$00
PositionNewRandom:
  INX
  LDA #$00
  CMP $00,X
  BNE PositionNewRandom
  CPY $46
  BEQ PositionNewRandomEnd
  INY
  JMP PositionNewRandom
PositionNewRandomEnd:
  STX $52                ; store position of new random block
  LDX #$01
  LDA $42
  ADC $52                ; additional randomization
  AND #%00000011
  BNE Create2Skip
  LDX #$02
Create2Skip:
  STX $50                ; store value of new random block
  TXA
  LDY $52
  STA $00,Y
  RTS

People:
  LDA $A2
  BNE BeginPeople
  RTS
BeginPeople:
  LDY $B5
  LDA [$EA],Y
  CMP $A1
  BNE SkipBankSwitchL
  INC $B5
  INC $B5
  INY
  LDA [$EA],Y
  TAX
  LDA #$44
  STA $8000
  STX $8001
SkipBankSwitchL:
  LDY $B6
  LDA [$EC],Y
  CMP $A1
  BNE SkipBankSwitchR
  INC $B6
  INC $B6
  INY
  LDA [$EC],Y
  TAX
  LDA #$45
  STA $8000
  STX $8001
SkipBankSwitchR:
  RTS

Press:
  JSR ClearFlagsAndTravel
  LDX #$00
  STX $49
Rotate1:
  JSR Rotate
  INC $49
  LDY $49
  CPY $50
  BNE Rotate1
  JSR ShiftL
Rotate2:
  LDX #$00
  JSR Rotate
  LDX #$20
  JSR Rotate
  INC $49
  LDY $49
  CPY #$04
  BNE Rotate2
  INC $40
  RTS

LatchController:
  LDA #$01
  STA $4016
  LDA #$00
  STA $4016
  LDX #$08
ReadControllerLoop:
  LDA $4016
  LSR A
  ROL $FF
  DEX
  BNE ReadControllerLoop
  RTS

ReadController:
  JSR LatchController
Reread:
  LDA $FF
  PHA
  JSR LatchController
  PLA
  CMP $FF
  BNE Reread
  LDA $FF
  BNE ReadGuard
  LDA #$00
  STA $40
  RTS
ReadGuard:
  LDA $40
  BNE ReadControllerExit
  LDA $41
  BNE ReadControllerExit
  LDA $47
  BNE ReadControllerExit
  LDA $48
  BEQ ReadSt
ReadControllerExit:
  RTS
ReadSt:
  LDA $FF
  CMP #$10
  BNE ReadSe
  LDA #$01
  STA $40
  STA $A2
  LDA #$00
  LDX #$00
BlankLoop:
  CLC
  ADC $00,X
  INX
  CPX #$10
  BNE BlankLoop
  CMP #$00
  BNE AnimateSkip
  JSR NewBlock
  JSR NewBlock
  JSR UpdateTiles
  RTS
AnimateSkip:
  LDY #$10
  STY $48
  RTS
ReadSe:
  LDA $FF
  CMP #$20
  BNE ReadR
  INC $40
  LDA $A2
  BEQ ReadControllerExit
  LDA $A3
  BEQ TurnMusicOn
  DEC $A3
  LDA #$00
  STA $4015
  JMP ReadR
TurnMusicOn:
  INC $A3
  LDA #$0F
  STA $4015
  LDA #$5E
  STA $4002
  LDA #$00
  STA $4003
ReadR:
  LDA $FF
  CMP #$01
  BNE ReadL
  LDA #$02
  STA $50
  JSR Press
  JSR ReverseTravel
ReadL:
  LDA $FF
  CMP #$02
  BNE ReadD
  JSR ClearFlagsAndTravel
  JSR ShiftL
  INC $40
ReadD:
  LDA $FF
  CMP #$04
  BNE ReadU
  LDA #$01
  STA $50
  JSR Press
  JSR ReverseTravel
  JSR TransferTravel
ReadU:
  LDA $FF
  CMP #$08
  BNE ReadControllerEnd
  LDA #$03
  STA $50
  JSR Press
  JSR TransferTravel
ReadControllerEnd:
  RTS

ReverseTravel:
  LDX #$00
ReverseTravelLoop:
  LDA #$00
  SEC
  SBC $20,X
  STA $20,X
  INX
  CPX #$10
  BNE ReverseTravelLoop
  RTS

Rotate:
  LDA $00,X
  TAY
  LDA $0C,X
  STA $00,X
  LDA $0F,X
  STA $0C,X
  LDA $03,X
  STA $0F,X
  TYA
  STA $03,X
  LDA $01,X
  TAY
  LDA $08,X
  STA $01,X
  LDA $0E,X
  STA $08,X
  LDA $07,X
  STA $0E,X
  TYA
  STA $07,X
  LDA $02,X
  TAY
  LDA $04,X
  STA $02,X
  LDA $0D,X
  STA $04,X
  LDA $0B,X
  STA $0D,X
  TYA
  STA $0B,X
  LDA $05,X
  TAY
  LDA $09,X
  STA $05,X
  LDA $0A,X
  STA $09,X
  LDA $06,X
  STA $0A,X
  TYA
  STA $06,X
  RTS

ScoreBuild:
  LDA $51
  SEC
  SBC #$02
  ASL A
  ASL A
  TAX
  LDY #$00
ScoresLoop:
  LDA scores,X
  STA $61,Y
  INX
  INY
  CPY #$04
  BNE ScoresLoop
  LDX #$60
  JSR AddAndRollover
  RTS

ShiftL:
  LDX #$70
  JSR ClearScore
  LDX #$00
EachRow:
  STX $50
  LDY #$00
  JSR Switch
  TYA
  STA $21,X
  INX
  LDY #$00
  JSR Switch
  DEX
  JSR Switch
  TYA
  STA $22,X
  INX
  INX
  LDY #$00
  JSR Switch
  DEX
  JSR Switch
  DEX
  JSR Switch
  TYA
  STA $23,X
  INX
  INX
  INX
  INX
  CPX #$10
  BNE EachRow
  LDX #$70
  JSR AddAndRollover
  JSR HighScore
  RTS

Switch:
  LDA #$00
  CMP $00,X
  BNE NotBlank
  CMP $01,X
  BEQ SwitchEnd
  LDA $01,X
  STA $00,X
  LDA #$00
  STA $01,X
  DEY
  DEY
  DEY
  LDA #$06
  STA $41
  RTS
NotBlank:
  LDA $30,X
  CMP #$01
  BEQ SwitchEnd
  LDA $00,X
  CMP $01,X
  BNE SwitchEnd
  INC $00,X
  LDA $00,X
  STA $51
  STX $4A
  STY $4B
  JSR ScoreBuild
  LDX $4A
  LDY $4B
  LDA #$00
  STA $01,X
  DEY
  DEY
  DEY
  LDA #$06
  STA $41
  LDA #$01
  STA $30,X
  CPX $50
  BEQ SwitchEnd
  INC $50
  STA $2F,X
  CPX $50
  BEQ SwitchEnd
  STA $2E,X
SwitchEnd:
  RTS

TransferPPU:
  LDA $4F
  BNE BeginTransferPPU
  RTS
BeginTransferPPU:
  LDX $56
  LDA PPU_Program0,X
  STA $AA
  LDA PPU_Program1,X
  STA $AB
  LDY $53
  LDA [$AA],Y
  STA $DA
  INY
  STY $53
  LDY #$00
  LDA [$DA],Y
  STA $BA
  INY
  LDA [$DA],Y
  STA $BB
  LDY $53
  LDA [$AA],Y
  STA $54
  INY
  LDA [$AA],Y
  STA $55
  INY
TransferPPULoop:
  LDA [$AA],Y
  CMP #$FF
  BNE ContinueTransferPPU
  DEC $4F
  LDA #$00
  STA $53
  STA $54
  RTS
ContinueTransferPPU:
  LDA $2002
  LDA [$AA],Y
  STA $2006
  INY 
  LDA [$AA],Y
  STA $2006
  INY
  LDA [$AA],Y
  TAX
  INY
  STY $53
  LDY $54
SameLine:
  LDA [$BA],Y
  STA $2007
  INY
  DEX
  BNE SameLine
  STY $54
  LDY $53
  DEC $55
  BNE TransferPPULoop
  RTS

TransferTravel:
  LDX #$00
TransferTravelLoop:
  LDA $20,X
  STA $10,X
  LDA #$00
  STA $20,X
  INX
  CPX #$10
  BNE TransferTravelLoop
  RTS

UpdatePPU:
  LDA #$00
  STA $2003
  LDA #$02
  STA $4014

  LDA $2002
  LDA #$3F
  STA $2006
  LDA #$09
  STA $2006
  LDX $A0
  LDA TilesR,X
  BNE LoadBrown
  LDA #$38
  STA $2007
  LDA #$21
  STA $2007
  JMP MouthStart
LoadBrown:
  LDA #$10
  STA $2007
  LDA #$17
  STA $2007
MouthStart:
  LDA $F7
  BEQ MouthsDone
  DEC $F7
  LDA $F1
  CMP #$FF
  BEQ Mouth1
  LDA $2002
  LDA #$21
  STA $2006
  LDA $F1
  STA $2006
  LDA $F2
  STA $2007
Mouth1:
  LDA $F3
  CMP #$FF
  BEQ MouthsDone
  LDA $2002
  LDA #$21
  STA $2006
  LDA $F3
  STA $2006
  LDA $F4
  STA $2007
MouthsDone:
  LDA $4C
  BEQ ScoreSkip
  DEC $4C
  LDA $2002
  LDA #$20
  STA $2006
  LDA #$B0
  STA $2006
  LDX $80
  INX
  STX $2007
  LDX $81
  INX
  STX $2007
  LDX $82
  INX
  STX $2007
  LDX $83
  INX
  STX $2007
  LDX $84
  INX
  STX $2007
  LDX #$59
  STX $2007
  LDX #$57
  STX $2007
  LDX $6000
  INX
  STX $2007
  LDX $6001
  INX
  STX $2007
  LDX $6002
  INX
  STX $2007
  LDX $6003
  INX
  STX $2007
  LDX $6004
  INX
  STX $2007
ScoreSkip:
  LDA $2002
  LDA #$20
  STA $2006
  LDA #$4F
  STA $2006
  LDA $4D
  BEQ SkipPlus
  LDA $79
  STA $2007
  LDA $7A
  STA $2007
  LDA $7B
  STA $2007
  LDA $7C
  STA $2007
  LDA $7D
  STA $2007
  LDA $7E
  STA $2007
  JMP PlusComplete
SkipPlus:
  LDA #$00
  STA $2007
  STA $2007
  STA $2007
  STA $2007
  STA $2007
  STA $2007
PlusComplete:
  LDA #%10010000   ; enable NMI, sprites from Pattern Table 0, background from Pattern Table 1
  STA $2000
  LDA #%00011110   ; enable sprites, enable background, no clipping on left side
  STA $2001
  LDA #$00         ; tell the ppu there is no background scrolling
  STA $2005
  STA $2005
  RTS

UpdateScore:
  LDA #$00
  LDY #$FF
  CMP $74
  BEQ PlusPosition4
  LDY #$04
PlusPosition4:
  CMP $73
  BEQ PlusPosition3
  LDY #$03
PlusPosition3:
  CMP $72
  BEQ PlusPosition2
  LDY #$02
PlusPosition2:
  CMP $71
  BEQ PlusPosition1
  LDY #$01
PlusPosition1:
  CMP $70
  BEQ PlusPosition0
  LDY #$00
PlusPosition0:
  CPY #$FF
  BNE PlusPositionDone
  RTS
PlusPositionDone:
  LDA #$00
  STA $79
  STA $7A
  STA $7B
  STA $7C
  STA $7D
  LDA #$10
  STA $79,Y
PlusLoop:
  LDA $70,Y
  CLC
  ADC #$11
  STA $7A,Y
  INY
  CPY #$05
  BNE PlusLoop
  LDA #$40
  STA $4D
  INC $4C
  RTS

UpdateTiles:
  LDX #$00
LoadBlock:
  STX $52
  LDA $00,X
  STA $50
  JSR IndividualTile
  LDX $52
  INX
  CPX #$10
  BNE LoadBlock
  RTS

  .bank 14
  .org $8000

