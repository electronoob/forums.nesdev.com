  .inesmap 5   ; mapper 5 = MMC3
  .inesprg 8   ; # of 8KB PRG banks 64KB
  .ineschr 64  ; # of 4KB CHR banks 256KB
  .inesmir 1   ; background mirroring

  .bank 15
  .org $FFFA
  .dw NMI
  .dw RESET
  .dw 0

controller = $FF
screen = $00
scroll = $01
lastscroll = $02
block = $03
blocktemp = $04
gamestate = $05
startguard = $06
drawfieldflag = $07
PPUCTRL = $08
counterA = $0D
counterB = $0E
attribtowrite = $0F
run0 = $10
run1 = $11
run = $12
extendedRAM0 = $20
extendedRAM1 = $21
bufferflag = $30
strip0 = $60
strip1 = $61
  
  .org $E000

RESET:
  LDA #$03
  STA $5101   ; CHR mode 8 separate 1K banks
  LDA #$01
  STA $5104   ; extended RAM mode 1
  LDA #$F4
  STA $5105   ; nametable mapping
  LDA #$00
  STA $5124   ; bank 00 mapped to PPU $1000-$13FF
  LDA #$01
  STA $5125   ; bank 01 mapped to PPU $1400-$17FF
  LDA #$02
  STA $5126   ; bank 02 mapped to PPU $1800-$1BFF
  LDA #$03
  STA $5127   ; bank 03 mapped to PPU $1C00-$1FFF
  LDA #$01
  STA $5203   ; IRQ counter
  SEI         ; disable IRQs
  CLD         ; disable decimal mode
  LDX #$40
  STX $4017   ; disable APU frame IRQ
  LDX #$FF
  TXS         ; Set up stack
  INX
  STX $2000   ; disable NMI
  STX $2001   ; disable rendering
  STX $4010   ; disable DMC IRQs

ClearMemory:
  LDA #$00
  STA $0000,X
  STA $0100,X
  STA $0300,X
  STA $0400,X
  STA $0500,X
  STA $0600,X
  STA $0700,X
  LDA #$FF
  STA $0200,X
  INX
  BNE ClearMemory

  LDA #%00010000
  STA PPUCTRL
  LDA #$01
  STA screen
  LDA #$80
  STA scroll
  JSR DrawField

Forever:
  JMP Forever

NMI:
  JSR UpdatePPU
  JSR ReadController
  JSR Rendering
  RTI
  
ExtendedRAMColumnWrite:
  LDX blocktemp
  LDA Field,X
  TAY
  LDA Strip1,Y
  STA run1
  LDA Strip0,Y
  CLC
  ADC #$19
  STA run0
  LDA run1
  ADC #$00
  STA run1
  LDY #$00
  STY run
NextRun:
  LDY run
  LDA [run0],Y
  AND #$1F
  STA counterB
  LDA [run0],Y
  AND #$C0
  STA attribtowrite
  LDY #$00
.RowLoop:
  LDA attribtowrite
  STA [extendedRAM0],Y
  LDA extendedRAM0
  CLC
  ADC #$20
  STA extendedRAM0
  LDA extendedRAM1
  ADC #$00
  STA extendedRAM1
  DEC counterB
  BNE .RowLoop
  LDY run
  INC run
  LDA [run0],Y
  AND #$20
  BNE NextRun
  RTS
  
Rendering:
  BIT $5204   ; IRQ status
  BVC Rendering
  LDA bufferflag
  BNE UpdateAttributesColumn
  RTS
UpdateAttributesColumn:
  LDA $0300
  STA blocktemp
  JSR BlocktempToExtendedRAM
  JSR ExtendedRAMColumnWrite
  RTS

BlocktempToExtendedRAM:
  LDA blocktemp
  AND #$1F
  CLC
  ADC #$80
  STA extendedRAM0
  LDA #$5C
  STA extendedRAM1
  RTS
  
BlocktempToPPUAddress:
  LDA $2002
  LDA blocktemp
  AND #%00100000
  LSR A
  LSR A
  LSR A
  CLC
  ADC #$20
  STA $2006
  LDA blocktemp
  AND #%00011111
  CLC
  ADC #$80
  STA $2006
  RTS

BlocktempToStripAddress:
  LDX blocktemp
  LDA Field,X
  TAY
  LDA Strip0,Y
  STA strip0
  LDA Strip1,Y
  STA strip1
  RTS

BufferToPPU:

  RTS
  
OneColumnToBuffer:
  INC bufferflag
  JSR BlocktempToStripAddress
  LDY #$00
.Loop:
  LDA [strip0],Y
  STA $0301,Y
  INY
  CPY #$19
  BNE .Loop
  RTS
  
ScreenScrollToBlock:
  LDA screen   ; turns screen and Scroll into a block number
  ASL A
  ASL A
  ASL A
  ASL A
  ASL A
  STA block
  LDA scroll
  LSR A
  LSR A
  LSR A
  CLC
  ADC block
  STA block
  RTS

LatchController:
  LDA #$01
  STA $4016
  LDA #$00
  STA $4016
  LDX #$08
.ReadControllerLoop:
  LDA $4016
  LSR A
  ROL controller
  DEX
  BNE .ReadControllerLoop
  RTS

ReadController:
  JSR LatchController
.Reread:
  LDA controller
  PHA
  JSR LatchController
  PLA
  CMP controller
  BNE .Reread
  LDA controller
  BNE .Skip0
  LDA #$00
  STA startguard
.Skip0:
  LDA gamestate
  BNE .Skip1
  JSR ControllerField
  RTS
.Skip1:
  JSR ControllerMenu
  RTS
  
ControllerField:  
  LDA scroll
  AND #%11111000
  STA lastscroll
  LDA controller
  CMP #$01
  BNE .ReadL
  LDA screen
  CMP #$03
  BNE .ReadR
  RTS
.ReadR:
  INC scroll
  INC scroll
  INC scroll
  INC scroll
  BNE .Skip0
  INC screen
.Skip0:
  LDA scroll
  AND #%11111000
  CMP lastscroll
  BNE .Skip1
  RTS
.Skip1:
  INC block
  LDA block
  CLC
  ADC #$20
  STA $0300
  STA blocktemp
  JSR OneColumnToBuffer
  RTS
.ReadL:
  CMP #$02
  BNE .ReadStart
  LDA scroll
  BNE .LeftScrollAllowed
  LDA screen
  BNE .LeftScrollAllowed
  RTS
.LeftScrollAllowed:
  LDA scroll
  BNE .Skip2
  DEC screen
.Skip2:
  DEC scroll
  DEC scroll
  DEC scroll
  DEC scroll
  LDA scroll
  AND #%11111000
  CMP lastscroll
  BEQ ReadEnd
  DEC block
  LDA block
  STA $0300
  STA blocktemp
  JSR OneColumnToBuffer
  RTS
.ReadStart:
  CMP #$10
  BNE ReadEnd
  LDA startguard
  BNE ReadEnd
  INC gamestate
  LDA #$01
  STA startguard
  JSR DrawMenu
ReadEnd: ; .ReadEnd doesn't work for some reason
  RTS
  
ControllerMenu:
  LDA controller
  CMP #$10
  BNE .ReadEnd
  LDA startguard
  BNE .ReadEnd
  DEC gamestate
  LDA #$01
  STA startguard
  INC drawfieldflag
.ReadEnd:
  RTS
  
PPUoff:
  LDA #$00
  STA $2001
  RTS
  
PPUon:
  LDA #%00011110
  STA $2001
  RTS
  
NMIoff:
  LDA PPUCTRL
  AND #%01111111
  STA PPUCTRL
  STA $2000
  RTS
  
NMIon:
  LDA PPUCTRL
  ORA #%10000000
  STA PPUCTRL
  STA $2000
  RTS
  
PPUacross:
  LDA PPUCTRL
  AND #%11111011
  STA PPUCTRL
  STA $2000
  RTS
  
PPUdown:
  LDA PPUCTRL
  ORA #%00000100
  STA PPUCTRL
  STA $2000
  RTS
  
DrawField:
  JSR NMIoff
  JSR PPUacross
  JSR PPUoff
  LDA $2002
  LDA #$3F
  STA $2006
  LDA #$00
  STA $2006
  LDX #$00
.PalettesLoop:
  LDA Palette,X
  STA $2007
  INX
  CPX #$20            
  BNE .PalettesLoop
  LDA $2002
  LDA #$20
  STA $2006
  LDA #$00
  STA $2006
  LDA #$01
  LDX #$80
.Black0Loop:
  STA $2007
  DEX
  BNE .Black0Loop
  LDA $2002
  LDA #$24
  STA $2006
  LDA #$00
  STA $2006
  LDA #$01
  LDX #$80
.Black1Loop:
  STA $2007
  DEX
  BNE .Black1Loop
  
  JSR PPUdown
  JSR PPUon
  
  JSR ScreenScrollToBlock
  STA blocktemp
  LDA #$21
  STA counterA
ColumnLoopTiles:
  JSR BlocktempToStripAddress
  JSR BlocktempToPPUAddress
  LDY #$00
.RowLoop:
  LDA [strip0],Y
  STA $2007
  INY
  CPY #$19
  BNE .RowLoop
  INC blocktemp
  DEC counterA
  BNE ColumnLoopTiles
IRQLoop0:
  BIT $5204   ; IRQ status
  BVC IRQLoop0
  LDA block
  STA blocktemp  
  LDA #$20
  STA counterA
ColumnLoopAttributes:
  JSR BlocktempToExtendedRAM
  JSR ExtendedRAMColumnWrite  
  INC blocktemp
  DEC counterA
  BNE ColumnLoopAttributes

  JSR NMIon
  JSR PPUacross
  JSR PPUon
  RTS
  
DrawMenu:
  JSR NMIoff
  JSR PPUacross
  JSR PPUoff
  LDA $2002
  LDA #$20
  STA $2006
  LDA #$00
  STA $2006
  TAX
  LDY #$04
.BlankLoop:
  STA $2007
  DEX
  BNE .BlankLoop
  DEY
  BNE .BlankLoop
  LDA #$00
  STA $A0
  LDA #$F8
  STA $A1
  LDY #$00
.DataLoop0:
  LDA $2002
  LDA [$A0],Y
  STA $2006
  JSR DataLoopNext
  STA $2006
  JSR DataLoopNext
  TAX
.DataLoop1:
  JSR DataLoopNext
  STA $2007
  DEX
  BNE .DataLoop1
  JSR DataLoopNext
  CMP #$FF
  BNE .DataLoop0
  JSR NMIon
  JSR PPUon
  RTS
  
DataLoopNext:
  INC $A0
  BNE .Skip
  INC $A1
.Skip:
  LDA [$A0],Y
  RTS
  
VBlankWait:
  BIT $2002
  BPL VBlankWait
  RTS

UpdatePPU:
  LDA #$00
  STA $2003
  LDA #$02
  STA $4014

  LDA bufferflag
  BEQ .SkipBuffer
  DEC bufferflag
  LDA $0300
  STA blocktemp
  JSR BlocktempToPPUAddress
  LDY #$00
.Loop:
  LDA $0301,Y
  STA $2007
  INY
  CPY #$19
  BNE .Loop
  
.SkipBuffer:
  JSR PPUon

  LDA drawfieldflag
  BEQ .Skip
  DEC drawfieldflag
  JSR DrawField
.Skip:
  LDA gamestate
  BNE .PPUMenu
  LDA screen
  CLC
  AND #$01
  ADC #%10010100
  STA $2000
  LDA scroll
  STA $2005
  LDA #$00
  STA $2005
  RTS
.PPUMenu:
  JSR NMIon
  LDA #$00
  STA $2005
  STA $2005
  RTS
  
  .bank 15
  .org $F800
  
  .db $3F,$10,$01, $0C   ; menu data
  .db $3F,$07,$01, $28
  .db $20,$63,$15, $A2,$9A,$98,$94,$A2,$8A,$00,$95,$90,$98,$A1,$00,$A1,$90,$98,$9D,$00,$A2,$9D,$9E,$A6
  .db $20,$A4,$05, $A6,$98,$9D,$93,$8A
  .db $20,$E4,$15, $A3,$94,$90,$9C,$8A,$00,$91,$A4,$95,$00,$91,$90,$9B,$00,$97,$9E,$A4,$00,$93,$94,$9D
  .db $21,$0A,$0E, $9C,$98,$90,$00,$92,$98,$9D,$00,$98,$9D,$93,$00,$9A,$92
  .db $21,$2A,$0F, $9D,$94,$00,$00,$92,$9B,$94,$00,$99,$90,$A7,$00,$9E,$90,$9A
  .db $21,$4A,$0E, $9D,$A8,$99,$00,$9F,$98,$A3,$00,$A3,$94,$9D,$00,$A2,$93
  .db $21,$8A,$0F, $93,$90,$9B,$00,$92,$97,$98,$00,$90,$A3,$9B,$00,$90,$A1,$98
  .db $21,$AA,$0F, $9D,$A8,$96,$00,$93,$94,$A3,$00,$92,$90,$A1,$00,$A2,$94,$90
  .db $21,$CA,$0E, $9F,$97,$98,$00,$96,$91,$00,$00,$9D,$9E,$00,$00,$A2,$95
  .db $21,$EA,$0F, $A6,$90,$A2,$00,$9C,$98,$9D,$00,$A3,$91,$00,$00,$A2,$A3,$9B
  .db $22,$23,$0B, $94,$A7,$A3,$A1,$90,$8A,$00,$9D,$9E,$9D,$94
  .db $22,$4A,$10, $91,$90,$92,$9A,$00,$A3,$9E,$00,$95,$9E,$9E,$A3,$91,$90,$9B,$9B
  .db $22,$6A,$0B, $9D,$95,$9B,$00,$9F,$9B,$90,$A8,$00,$86,$80
  .db $22,$8A,$12, $91,$92,$00,$90,$A6,$90,$A1,$94,$9D,$94,$A2,$A2,$00,$9C,$9E,$9D,$A3,$97
  .db $22,$AA,$11, $A2,$90,$9B,$A4,$A3,$94,$00,$A3,$9E,$00,$A2,$94,$A1,$A5,$98,$92,$94
  .db $22,$CA,$0C, $A3,$97,$90,$9D,$9A,$A2,$96,$98,$A5,$98,$9D,$96
  .db $22,$EA,$09, $A6,$98,$9B,$93,$00,$92,$90,$A1,$93
  .db $23,$0A,$0A, $93,$98,$A5,$98,$A2,$98,$9E,$9D,$90,$9B
  .db $23,$2A,$0C, $92,$97,$90,$9C,$9F,$98,$9E,$9D,$A2,$97,$98,$9F
  .db $23,$4A,$0A, $A2,$A4,$9F,$94,$A1,$00,$91,$9E,$A6,$9B
  .db $FF

Palette:
  .db $FF,$39,$0D,$30, $FF,$26,$0D,$30, $FF,$39,$01,$30, $FF,$00,$00,$30   ; Background
  .db $1A,$00,$00,$00, $FF,$00,$00,$00, $FF,$00,$00,$00, $FF,$00,$00,$00   ; Sprites
  
Field:
  .db $00,$00,$09,$0A                           ; LOutside
  .db $0B,$01,$01,$01,$01,$01,$01,$01,$01,$0C   ; LEndzone
  .db $0D,$02,$03,$02,$04,$05,$02,$02,$0E,$0F   ; 0-10
  .db $10,$11,$02,$02,$04,$05,$02,$02,$12,$13   ; 10-20
  .db $14,$15,$02,$02,$04,$05,$02,$02,$16,$17   ; 20-30
  .db $18,$19,$02,$02,$06,$07,$02,$02,$1A,$1B   ; 30-40
  .db $1C,$1D,$02,$02,$04,$05,$02,$02,$1E,$1F   ; 40-50
  .db $20,$21,$02,$02,$04,$05,$02,$02,$22,$23   ; 50-40
  .db $24,$25,$08,$02,$06,$07,$02,$02,$26,$27   ; 40-30
  .db $28,$29,$08,$02,$04,$05,$02,$02,$2A,$2B   ; 30-20
  .db $2C,$2D,$08,$02,$04,$05,$02,$02,$2E,$2F   ; 20-10
  .db $30,$31,$08,$02,$04,$05,$02,$02,$03,$32   ; 10-0
  .db $33,$01,$01,$01,$01,$01,$01,$01,$01,$34   ; REndzone
  .db $35,$36,$00,$00                           ; ROutside

Strip0:
  .db $00,$1A,$34,$4E,$68,$82,$9C,$B6,$D0,$EA,$05,$1F,$39,$53,$6D,$87
  .db $A1,$BB,$D5,$EF,$09,$23,$3D,$57,$71,$8B,$A5,$BF,$D9,$F3,$0D,$27
  .db $42,$5D,$77,$91,$AB,$C5,$DF,$F9,$13,$2D,$47,$61,$7B,$95,$AF,$C9
  .db $E3,$FD,$17,$31,$4B,$65,$80
  
Strip1:
  .db $F0,$F0,$F0,$F0,$F0,$F0,$F0,$F0,$F0,$F0,$F1,$F1,$F1,$F1,$F1,$F1
  .db $F1,$F1,$F1,$F1,$F2,$F2,$F2,$F2,$F2,$F2,$F2,$F2,$F2,$F2,$F3,$F3
  .db $F3,$F3,$F3,$F3,$F3,$F3,$F3,$F3,$F4,$F4,$F4,$F4,$F4,$F4,$F4,$F4
  .db $F4,$F4,$F5,$F5,$F5,$F5,$F5
  
  .bank 15
  .org $F000
  
  .db $00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00, $19       ; 00 BlankOutside

  .db $00,$00,$03,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$00,$03, $19       ; 01 BlankEndzone

  .db $00,$00,$03,$42,$00,$00,$00,$00,$00,$00,$00,$44,$00,$00,$00,$47,$00,$00,$00,$00,$00,$00,$00,$49,$03, $19       ; 02 Y
  
  .db $00,$00,$03,$42,$00,$00,$00,$00,$00,$00,$00,$44,$00,$43,$00,$47,$00,$00,$00,$00,$00,$00,$00,$49,$03, $19       ; 03 2Y

  .db $00,$00,$03,$42,$00,$00,$00,$00,$00,$00,$00,$45,$00,$00,$00,$48,$00,$00,$00,$00,$00,$00,$00,$49,$03, $19       ; 04 5Y
