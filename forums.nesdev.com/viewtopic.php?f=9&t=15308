p; &nbsp;cpu_write(d, 0xa000, i);<br />&nbsp; &nbsp;&nbsp; &nbsp;cpu_read(d, 0x8000, banksize);<br />&nbsp; &nbsp;}<br />&nbsp; &nbsp;cpu_read(d, 0xc000, banksize);<br />}<br /><br />/*<br />PPU ã® read é€”ä¸­ã«ãƒãƒ³ã‚¯ãƒ¬ã‚¸ã‚¹ã‚¿ãŒåˆ‡ã‚Šæ›¿ã‚ã‚‹ã‚‰ã—ã„ã®ã§ä¸‹è¨˜ã®å‡¦ç†ã§<br />åŒã˜ãƒ‡ãƒ¼ã‚¿ã‚’å¾—ã‚‹ã‚ˆã†ã«ã™ã‚‹ã€‚<br />PPU address&nbsp; &nbsp; &nbsp; register<br />0x0000-0x0fdf -&gt; #0<br />0x0fe0-0x0fff -&gt; #1<br />0x1000-0x1fdf -&gt; #2<br />0x1fe0-0x1fff -&gt; #3<br /><br />ppu_read å‰ã« #0 + #1 , #2 + #3 ã®å†…å®¹ã¯åŒã˜ã«ã—ã¦ãŠãã€‚<br />*/<br />function ppu_dump(d, pagesize, banksize)<br />{<br />&nbsp; &nbsp;for(local i = 0; i &lt; pagesize; i += 2){<br />&nbsp; &nbsp;&nbsp; &nbsp;ppu_read(d, 0x0fd0, 0);<br />&nbsp; &nbsp;&nbsp; &nbsp;cpu_write(d, 0xb000, i);<br />&nbsp; &nbsp;&nbsp; &nbsp;ppu_read(d, 0x0fe0, 0);<br />&nbsp; &nbsp;&nbsp; &nbsp;cpu_write(d, 0xc000, i);<br /><br />&nbsp; &nbsp;&nbsp; &nbsp;ppu_read(d, 0x1fd0, 0);<br />&nbsp; &nbsp;&nbsp; &nbsp;cpu_write(d, 0xd000, i + 1);<br />&nbsp; &nbsp;&nbsp; &nbsp;ppu_read(d, 0x1fe0, 0);<br />&nbsp; &nbsp;&nbsp; &nbsp;cpu_write(d, 0xe000, i + 1);<br /><br />&nbsp; &nbsp;&nbsp; &nbsp;ppu_read(d, 0, banksize * 2);<br />&nbsp; &nbsp;}<br />}<br /><br />function cpu_ram_access(d, pagesize, banksize)<br />{<br />&nbsp; &nbsp;cpu_ramrw(d, 0x6000, banksize);<br />}</div></div>

