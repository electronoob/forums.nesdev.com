<br />
<br />txa ; x=0
<br />clrmem:      
<br />    sta $000,x  ; a = o
<br />    sta $100,x
<br />    sta $200,x
<br />    sta $300,x
<br />    sta $400,x
<br />    sta $500,x
<br />    sta $600,x
<br />    sta $700,x  ; Remove this if you're storing reset-persistent data
<br />    inx
<br />    bne clrmem
<br />   
<br />
